<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.shihp.mybatis.mapper.UserMapper">
    <select id="selectUserById" resultType="com.shihp.mybatis.entity.User">
        SELECT ID, NAME, AGE
        FROM TB_USER
        WHERE ID = #{id};
    </select>
</mapper>
